    "@angular/language-service": "^10.2.4",
    "@babel/core": "^7.9.6",
    "@datorama/akita-ngdevtools": "^4.0.0",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.1.0",
