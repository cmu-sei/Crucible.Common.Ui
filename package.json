    "build:clean": "rimraf dist",
    "build:link": "npm run build:clean && run-p watch:* link:crucible-common ",
    "json-server": "json-server ./projects/cwd-common-app/src/assets/db/db.json",
    "playground": "angular-playground"
  },
  "private": true,
