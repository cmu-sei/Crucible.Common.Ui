    "@babel/core": "^7.14.8",
    "@datorama/akita-ngdevtools": "^6.0.1",
    "@types/node": "^16.3.3",
    "babel-loader": "^8.2.2",
    "codelyzer": "^6.0.2",
    "json-server": "^0.17.1",
    "ng-packagr": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "ts-node": "~10.1.0",
