  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch:crucible-common": "ng build crucible-common --watch",
    "link:crucible-common": "wait-on dist/@crucible-common/package.json && npm link dist/@crucible-common",
